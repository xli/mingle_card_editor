{
  "name": "Mingle Card Editor",
  "version": "1.0",
  "manifest_version": 1,
  "description": "Change Mingle Card into inline editing mode.",
  "icons": { "16": "icon.png",
             "48": "icon.png",
            "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": [
         "jquery-1.7.2.js",
         "jquery-ui-1.8.18.custom.min.js",
         "underscore-1.3.1.js",
         "wikimate.js",
         "paragraph.js",
         "wiki_parser.js",
         "mingle_wikimate.js"
       ],
       "css": ["wikimate.css"]
     }
   ],
  "permissions": []
}