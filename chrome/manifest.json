{
  "name": "Mingle Card Editor",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Change Mingle Card into inline editing mode.",
  "update_url": "https://dl.dropbox.com/s/ieoc87jwaeqg0bm/updates.xml?dl=1",
  "icons": { "16": "icon.png",
             "48": "icon.png",
            "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": [
         "jquery-1.7.2.js",
         "jquery-ui-1.8.18.custom.min.js",
         "underscore-1.3.1.js",
         "diff.js",
         "wikimate-min.js",
         "wiki_parser.js",
         "wikimate_plugins.js",
         "mingle_wikimate.js"
       ],
       "css": ["wikimate.css", "style.css"]
     }
   ],
  "permissions": []
}